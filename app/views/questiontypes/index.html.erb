<h1>Listing questiontypes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questiontypes.each do |questiontype| %>
  <tr>
    <td><%= questiontype.name %></td>
    <td><%= questiontype.active %></td>
    <td><%= link_to 'Show', questiontype %></td>
    <td><%= link_to 'Edit', edit_questiontype_path(questiontype) %></td>
    <td><%= link_to 'Destroy', questiontype, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Questiontype', new_questiontype_path %>
